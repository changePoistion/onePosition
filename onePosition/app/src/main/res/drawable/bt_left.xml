<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:bottomLeftRadius="12dp" android:topLeftRadius="12dp" />
            <gradient android:endColor="@color/colorWhite" android:startColor="@color/colorWhite" />
            <stroke android:width="1dp" android:color="@color/colorQQBlue"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape>
            <corners android:bottomLeftRadius="12dp" android:topLeftRadius="12dp" />
            <gradient android:endColor="@color/colorQQBlue" android:startColor="@color/colorQQBlue" />
            <stroke android:width="1dp" android:color="@color/colorQQBlue"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:bottomLeftRadius="12dp" android:topLeftRadius="12dp" />
            <gradient android:endColor="@color/colorWhite" android:startColor="@color/colorWhite" />
            <stroke android:width="1dp" android:color="@color/colorQQBlue"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:bottomLeftRadius="12dp" android:topLeftRadius="12dp" />
            <gradient android:endColor="@color/colorQQBlue" android:startColor="@color/colorQQBlue" />
            <stroke android:width="1dp" android:color="@color/colorQQBlue"/>
        </shape>
    </item>
</selector>